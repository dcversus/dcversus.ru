{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],

    "no-console": "off"
  }
}
